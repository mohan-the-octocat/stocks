{
  "name": "Stocks Template", 
  "dockerComposeFile": "./docker-compose.yml",
  "service": "dev",
  "runServices": [
      "db"
  ],
"workspaceFolder": "/workspaces/stocks",
"postCreateCommand": "bash /workspaces/stocks/.devcontainer/postCreate.sh",
"customizations": {
	"vscode": {
		"extensions": [
			"ms-azuretools.vscode-docker",
			"formulahendry.vscode-mysql",
			"DEVSENSE.phptools-vscode"
		]
	}
}
}
